{
  "name": "klipper_dash_visualizer",
  "version": "0.0.1",
  "description": "A Klipper serial output visualizer plugin for Dash",
  "main": "build/index.js",
  "scripts": {
    "start": "webpack-serve --config ./webpack.serve.config.js --open",
    "validate-init": "python _validate_init.py",
    "prepublishOnly": "npm run validate-init",
    "build:js": "webpack --mode production",
    "build:js-dev": "webpack --mode development",
    "build:py_and_r": "dash-generate-components ./src/lib/components klipper_dash_visualizer -p package-info.json --r-prefix ''",
    "build:py_and_r-activated": "(. .venv/bin/activate || .venv\\scripts\\activate && npm run build:py_and_r)",
    "build": "npm run build:js && npm run build:py_and_r",
    "build-dev": "npm run build:js-dev && npm run build:py_and_r",
    "build:activated": "npm run build:js && npm run build:py_and_r-activated",
    "build-dev:activated": "npm run build:js-dev && npm run build:py_and_r-activated",
    "test": "jest",
    "clean": "git clean -xdf build dist inst klipper_dash_visualizer klipper_dash_visualizer.egg-info man R NAMESPACE DESCRIPTION"
  },
  "author": "Fred Sundvik <fsundvik@gmail.com>",
  "license": "MIT",
  "jest": {
    "rootDir": "src",
    "modulePaths": [
      "<rootDir>/lib"
    ],
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "babelConfig": true
      }
    },
    "setupFilesAfterEnv": [
      "jest-enzyme",
      "<rootDir>/lib/jest_setup.ts",
      "jest-extended"
    ],
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    }
  },
  "dependencies": {
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^4.9.14",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.53",
    "@material-ui/styles": "^4.9.14",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^26.0.4",
    "@types/jsdom": "^16.2.3",
    "@types/plotly.js": "^1.50.11",
    "@types/react": "16.9.41",
    "@types/react-dom": "16.9.8",
    "@types/react-plotly.js": "^2.2.4",
    "@types/sinon": "^9.0.4",
    "@types/tinycolor2": "^1.4.2",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.0.6",
    "copyfiles": "^2.1.1",
    "css-loader": "^3.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-react": "^7.14.2",
    "file-loader": "^6.0.0",
    "hamjest": "^3.7.1",
    "jest": "^26.1.0",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.2",
    "npm": "^6.1.0",
    "plotly.js": "^1.54.1",
    "prop-types": "15.7.2",
    "react": "16.13.0",
    "react-docgen": "^4.1.1",
    "react-dom": "16.13.0",
    "react-plotly.js": "^2.4.0",
    "sinon": "^9.0.2",
    "style-loader": "^0.23.1",
    "styled-jsx": "^3.2.1",
    "three": "0.115.0",
    "tinycolor2": "^1.4.1",
    "ts-jest": "^26.1.1",
    "ts-loader": "^7.0.0",
    "typeface-roboto": "0.0.75",
    "typescript": "^3.8.3",
    "webpack": "4.36.1",
    "webpack-cli": "3.3.6",
    "webpack-serve": "3.1.0"
  },
  "engines": {
    "node": ">=14.5.0",
    "npm": ">=6.1.0"
  }
}
